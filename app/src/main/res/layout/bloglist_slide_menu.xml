<?xml version="1.0" encoding="utf-8"?>
<com.android.rdc.rdcblog.bloglist.view.defined.SlidingMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rdc="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sliding_menu_contol"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:scrollbars="none"
    rdc:rightPadding="160dp"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <include
            layout="@layout/bloglist_slide_menu_item" />

        <com.android.rdc.rdcblog.bloglist.view.defined.MenuViewPager
            android:id="@+id/sliding_menu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            ></com.android.rdc.rdcblog.bloglist.view.defined.MenuViewPager>


    </LinearLayout>

</com.android.rdc.rdcblog.bloglist.view.defined.SlidingMenu>